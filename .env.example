# Discord bot token from https://discord.com/developers/applications
DISCORD_TOKEN=your_bot_token_here

# Target Discord channel ID (right click on channel -> Copy ID)
CHANNEL_ID=1381368509908652224

# Directory to store exported messages
DOWNLOAD_DIR=exports

# Delay between paginated requests in milliseconds
REQUEST_DELAY=1000

# Interval between sync runs in milliseconds (0 = single run)
SYNC_INTERVAL_MS=600000

# Stop the bot when нет новых сообщений после синхронизации
EXIT_ON_IDLE=true
